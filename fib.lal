# Fibbonaci assuming lambda self exists
(let 'fib
  (lambda 'x 'y
    (let 'n (+ x y)
      (list
        n
        (if (< n 100)
          (self y n)
        )
      )
    )
  )
  (list 0 1 (fib 0 1))
)
